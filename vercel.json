{
  "version": 2,
  "builds": [
    {
      "src": "src/**/*.{js,jsx}",
      "use": "@vercel/react"
    },
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/src/$1"
    }
  ]
}
